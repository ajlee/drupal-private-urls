<?php

function newsletter_access_install() {
    variable_set('newsletter_access_pageTitle', 'My TestFoo Page');
    variable_set('newsletter_access_node_types', 'Newsletter Access Node Types');
    variable_set('newsletter_access_key', 'Newsletter Access Key');
}

function testFoo_uninstall() {
    variable_del('newsletter_access_pageTitle');
    variable_del('newsletter_access_node_types');
}